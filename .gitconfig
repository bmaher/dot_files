[alias]
        l  = log --oneline
        st = status
        co = checkout
        gerrit = push origin HEAD:refs/for/master
        draft = push origin HEAD:refs/drafts/master
[pull]
	rebase = true
[push]
	default = simple
[core]
	autocrlf = input
	editor = vim
[color]
	ui = auto
